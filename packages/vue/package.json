{
  "name": "@elegant-router/vue",
  "version": "0.0.2",
  "description": "A Vue3 Router plugin to create router elegantly",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/elegant-router",
  "repository": {
    "url": "https://github.com/soybeanjs/elegant-router.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/elegant-router/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.mjs",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.mjs",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.mjs",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.mjs",
      "import": "./dist/esbuild.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typecheck && unbuild",
    "stub": "unbuild --stub",
    "watch": "tsx watch src",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "consola": "3.2.3",
    "fs-extra": "11.1.1",
    "kolorist": "1.8.0",
    "magic-string": "^0.30.2",
    "magicast": "0.2.10",
    "prettier": "^3.0.2",
    "unplugin": "1.4.0"
  },
  "devDependencies": {
    "@elegant-router/core": "workspace:*",
    "@types/fs-extra": "11.0.1",
    "@types/node": "20.5.1",
    "@vue/compiler-sfc": "^3.3.4",
    "typescript": "5.1.6",
    "unbuild": "1.2.1",
    "vite": "^4.4.9",
    "vue-router": "^4.2.4"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "3.3.4"
  }
}
