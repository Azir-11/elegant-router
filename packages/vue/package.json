{
  "name": "@elegent-router/vue",
  "version": "0.0.1",
  "description": "A Vue3 Router plugin to create router elegently",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/elegent-router",
  "repository": {
    "url": "https://github.com/soybeanjs/elegent-router.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/elegent-router/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.mjs",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.mjs",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.mjs",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.mjs",
      "import": "./dist/esbuild.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typecheck && unbuild",
    "dev": "unbuild --stub",
    "watch": "tsx watch src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "consola": "3.2.2",
    "fs-extra": "11.1.1",
    "kolorist": "1.8.0",
    "magicast": "0.2.9",
    "unplugin": "1.3.1"
  },
  "devDependencies": {
    "@elegent-router/core": "workspace:*",
    "@types/fs-extra": "11.0.1",
    "@types/node": "20.3.3",
    "typescript": "5.1.6",
    "unbuild": "1.2.1",
    "vite": "^4.3.9",
    "vue-router": "^4.2.2"
  }
}
