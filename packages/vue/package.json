{
  "name": "@elegant-router/vue",
  "version": "0.1.4",
  "description": "A Vue3 Router plugin to create router elegantly",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/elegant-router",
  "repository": {
    "url": "https://github.com/soybeanjs/elegant-router.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/elegant-router/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.mjs",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.mjs",
      "require": "./dist/esbuild.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typecheck && pnpm unbuild",
    "stub": "unbuild --stub",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@elegant-router/core": "workspace:*",
    "consola": "3.2.3",
    "kolorist": "1.8.0",
    "magic-string": "0.30.3",
    "magicast": "0.2.10",
    "prettier": "3.0.2",
    "unplugin": "1.4.0"
  },
  "devDependencies": {
    "@types/node": "20.5.1",
    "rollup": "3.28.0",
    "typescript": "5.1.6",
    "unbuild": "1.2.1",
    "vite": "4.4.9",
    "vue-router": "4.2.4"
  }
}
