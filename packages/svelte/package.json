{
  "name": "@elegant-router/svelte",
  "version": "0.0.2",
  "description": "A Svelte Router plugin to create router elegantly",
  "private": true,
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/elegant-router",
  "repository": {
    "url": "https://github.com/soybeanjs/elegant-router.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/elegant-router/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typecheck && unbuild",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "consola": "3.2.3",
    "fs-extra": "11.1.1",
    "magicast": "0.2.10",
    "unplugin": "1.4.0"
  },
  "devDependencies": {
    "@elegant-router/core": "workspace:*",
    "@types/fs-extra": "11.0.1",
    "@types/node": "20.5.1",
    "typescript": "5.1.6",
    "unbuild": "1.2.1"
  }
}
