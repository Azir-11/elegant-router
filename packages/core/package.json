{
  "name": "@elegant-router/core",
  "version": "0.0.1",
  "description": "A plugin to create router elegantly",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/elegant-router",
  "repository": {
    "url": "https://github.com/soybeanjs/elegant-router.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/elegant-router/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typecheck && unbuild",
    "dev": "unbuild --stub",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chokidar": "3.5.3",
    "consola": "3.2.3",
    "fast-glob": "3.3.0",
    "kolorist": "1.8.0",
    "micromatch": "4.0.5"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.2",
    "@types/node": "20.4.1",
    "typescript": "5.1.6",
    "unbuild": "1.2.1"
  }
}
