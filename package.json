{
  "name": "elegant-router",
  "type": "module",
  "version": "1.0.0-alpha.1",
  "packageManager": "pnpm@10.9.0",
  "description": "A plugin to create router elegantly",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/elegant-router",
  "repository": {
    "url": "https://github.com/soybeanjs/elegant-router.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/elegant-router/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.mjs",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.mjs",
      "require": "./dist/esbuild.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": ["./dist/*", "./*"]
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "unbuild",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "lint": "eslint . --fix",
    "prepare": "pnpm stub",
    "publish-pkg": "pnpm publish --access public",
    "release": "pnpm soy release",
    "stub": "unbuild --stub",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "update-pkg": "soy ncu"
  },
  "dependencies": {
    "chokidar": "4.0.3",
    "consola": "^3.4.2",
    "kolorist": "1.8.0",
    "picomatch": "4.0.2",
    "tinyglobby": "0.2.13"
  },
  "devDependencies": {
    "@soybeanjs/cli": "1.2.1",
    "@soybeanjs/eslint-config": "1.6.0",
    "@types/node": "22.14.1",
    "@types/picomatch": "4.0.0",
    "eslint": "9.25.1",
    "eslint-plugin-vue": "10.0.0",
    "lint-staged": "15.5.1",
    "simple-git-hooks": "2.12.1",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "unbuild": "3.5.0",
    "vue-eslint-parser": "10.1.3"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
